<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
     http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>BooksServlet</servlet-name>
    <servlet-class>com.prismo.BooksServlet</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>HttpTrace</servlet-name>
    <servlet-class>com.prismo.HttpTrace</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EncryptionServlet</servlet-name>
    <servlet-class>com.prismo.EncryptionServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MongoActionsServlet</servlet-name>
    <servlet-class>mongo.MongoActionsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AzureActionsServlet</servlet-name>
    <servlet-class>azure.AzureActionsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>XmlXXE</servlet-name>
    <servlet-class>com.prismo.XmlXXE</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BooksServlet1</servlet-name>
    <servlet-class>com.prismo.BooksServlet1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ExecuteServlet</servlet-name>
    <servlet-class>com.prismo.ExecuteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>OSCommandServlet</servlet-name>
    <servlet-class>com.prismo.OSCommandServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AppServlet</servlet-name>
    <servlet-class>com.prismo.AppServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AsyncServlet</servlet-name>
    <servlet-class>com.prismo.AsyncServlet</servlet-class>
    <async-supported>true</async-supported>
  </servlet>
  <servlet>
    <servlet-name>Deserialize1</servlet-name>
    <servlet-class>com.prismo.Deserialize1</servlet-class>
    <async-supported>true</async-supported>
  </servlet>
  <servlet>
    <servlet-name>NewSQLExitServlet</servlet-name>
    <servlet-class>com.prismo.NewSQLExitServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BotTest</servlet-name>
    <servlet-class>com.prismo.BotTest</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>elExpression</servlet-name>
    <servlet-class>com.prismo.ElExpression</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TypeSniff</servlet-name>
    <servlet-class>com.prismo.TypeSniff</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.prismo.FileUploadServlet</servlet-class>
  </servlet>
  <!-- 
  <servlet>
    <servlet-name>SecurePage</servlet-name>
    <servlet-class>com.prismo.SecurePage</servlet-class>
  </servlet>
   -->

  <servlet-mapping>
    <servlet-name>EncryptionServlet</servlet-name>
    <url-pattern>/encryption</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MongoActionsServlet</servlet-name>
    <url-pattern>/mongo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AzureActionsServlet</servlet-name>
    <url-pattern>/azure</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>XmlXXE</servlet-name>
    <url-pattern>/xmlxxe</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>elExpression</servlet-name>
    <url-pattern>/elexpression</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewSQLExitServlet</servlet-name>
    <url-pattern>/newsqlexit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BooksServlet</servlet-name>
    <url-pattern>/booklist</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>HttpTrace</servlet-name>
    <url-pattern>/httptrace</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BooksServlet1</servlet-name>
    <url-pattern>/booklist1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Deserialize1</servlet-name>
    <url-pattern>/deserialize1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ExecuteServlet</servlet-name>
    <url-pattern>/cmdexec</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>OSCommandServlet</servlet-name>
    <url-pattern>/oscmd</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AppServlet</servlet-name>
    <url-pattern>/ssrf</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AsyncServlet</servlet-name>
    <url-pattern>/async</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BotTest</servlet-name>
    <url-pattern>/bottest</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TypeSniff</servlet-name>
    <url-pattern>/typesniff</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/upload</url-pattern>
  </servlet-mapping>
  <!-- 
  <servlet-mapping>
    <servlet-name>SecurePage</servlet-name>
    <url-pattern>/securepage</url-pattern>
  </servlet-mapping>
   -->
  <filter>
    <filter-name>CSPFilter</filter-name>
    <filter-class>com.prismo.CSPFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CSPFilter</filter-name>
    <url-pattern>/typesniff</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>WrapperFilter</filter-name>
    <filter-class>com.prismo.wrapper.WrapperFilter</filter-class>
  </filter>

  <filter-mapping>
    <filter-name>WrapperFilter</filter-name>
    <url-pattern>/wrapper</url-pattern>
  </filter-mapping>



  <servlet>
    <servlet-name>Servlet</servlet-name>
    <servlet-class>com.prismo.wrapper.WrapperServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Servlet</servlet-name>
    <url-pattern>/wrapper</url-pattern>
  </servlet-mapping>

  <filter>
    <filter-name>httpHeaderSecurity</filter-name>
    <filter-class>org.apache.catalina.filters.HttpHeaderSecurityFilter</filter-class>
    <async-supported>true</async-supported>
    <init-param>
      <param-name>hstsMaxAgeSeconds</param-name>
      <param-value>31536000</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>httpHeaderSecurity</filter-name>
    <url-pattern>/hsts/securepage</url-pattern>
    <dispatcher>REQUEST</dispatcher>
  </filter-mapping>
  
  <security-constraint>
    <web-resource-collection>
      <web-resource-name>prismo-webapp</web-resource-name>
      <url-pattern>/hsts/*</url-pattern>
      <http-method>GET</http-method>
      <http-method>POST</http-method>
      </web-resource-collection>
      <user-data-constraint>
        <transport-guarantee>CONFIDENTIAL</transport-guarantee>
      </user-data-constraint>
  </security-constraint>

  <context-param>
    <param-name>MONGODB_HOST</param-name>
    <param-value>localhost</param-value>
  </context-param>
  <context-param>
    <param-name>MONGODB_PORT</param-name>
    <param-value>27017</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>persons.jsp</welcome-file>
  </welcome-file-list>
</web-app>

